<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <from>^/$</from>
        <to>/index</to>
    </rule>
    <rule>
        <from>^/(CRA[0-9]{6})/(CRR[0-9]{6})$</from>
        <to type="forward">/taxonomy/detail?gsaAcc=$1&amp;runAcc=$2</to>
    </rule>
</urlrewrite>

